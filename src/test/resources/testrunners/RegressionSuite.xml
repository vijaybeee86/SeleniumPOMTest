<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Open Cart Regression Suite" thread-count="2"
	parallel="tests" verbose="4">

<listeners>
	<!--<listener class-name="com.qa.ecomm.listeners.ExtentReportListener" />-->
	
	<listener class-name="com.qa.ecomm.listeners.TestAllureListener" />
	
	<listener class-name="com.qa.ecomm.listeners.AnnotationTransformer" />
	
</listeners>

	<test name="Login Page Test">

		<classes>
			<class name="com.qa.ecomm.tests.LoginPageTest" />
		</classes>
	</test>
	

	<test name="Forgot Password Test">
		<classes>
			<class name="com.qa.ecomm.tests.ForgotPasswordTest" />
		</classes>
	</test>


</suite>


